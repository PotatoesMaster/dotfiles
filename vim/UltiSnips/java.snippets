snippet ln "ln"
System.out.println(${1});
endsnippet

snippet err "err"
System.err.print(${1});
endsnippet

snippet wh "wh"
while (${1}) {
	${2};
}
endsnippet

snippet for "for"
for (${1:i}=0; ${1} ${2:<} ${3:10}; ${1}${4:++}) {
	${5};
}
endsnippet

snippet class "class"
${1:public }class ${2:`!v Filename("", "untitled")`}${3} {
	${4}
}
endsnippet


