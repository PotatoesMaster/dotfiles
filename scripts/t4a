#!/bin/bash

do_it() {
patterndo \
  "$HOME/downloads/quvi" \
  'Question de guitare (.*) ?\??\.mp4' \
  "$HOME"'/videos/web/guitare/# DÃ©butant/Questions de guitare/$1 ?.mp4' \
